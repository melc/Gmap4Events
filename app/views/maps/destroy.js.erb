// Delete a location
$("#btn_markerDel").click(function (e) {
    e.preventDefault();
    if (window.confirm('Are you sure you want to remove this location?'))
    {
        $.ajax({
            type: "DELETE",
            url: "/maps/7/destroy",
            success: function(msg) {
                console.log(msg);
            },
            failure: function(errMsg) {
                console.log(errMsg);
            }
        })
    }
});